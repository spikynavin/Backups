need to remove /etc/udev/rules.d/*

vi /etc/init.d/udev

#!/bin/bash

case "$1" in
  start)
    #echo "Starting udev"
    /lib/udev/udevd -d
    udevadm trigger --action=add
    udevadm settle
    ;;
  stop)
    echo "Stopping udevd"
    start-stop-daemon --stop --name udevd --quiet
    ;;
  restart)
    $0 stop
    sleep 1
    $0 start
    ;;
  status)
    pid=`pidof -x udevd`
    if [ -n "$pid" ]; then
        echo "udevd (pid $pid) is running ..."
    else
        echo "udevd is stopped"
    fi
    ;;
  *)
    echo "Usage: $0 {start|stop|status|restart}"
    exit 1
esac
exit 0
